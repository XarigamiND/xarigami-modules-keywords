<xar:if condition="isset($words) and count($words) gt 0">
<div class="xar-keywords-list">
    <span class="xar-title">
        #$label#:
    </span>
    <xar:foreach in="$words" value="$word">
        <a href="#$word['url']#">#$word['keyword']#</a>
    </xar:foreach>
</div>
</xar:if>

